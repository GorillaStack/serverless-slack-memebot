{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
      "arrow-parens": ["error", "as-needed"],
      "no-underscore-dangle": ["error", { "allow": ["_this"] }],
      "no-useless-escape": 0,
      "class-methods-use-this": 0,
      "import/no-named-as-default": 0,
      "max-len": ["error", 120]
    }
}
